/**index.wxss**/

.menubutton{
  position: fixed;
  right: 1rem;
  bottom: 30rpx;
}
.menubutton-button{
  border-radius: 50%;
  background: #08C;
  width: 50px;
  height: 50px;
  color: #FFF;
}
.code-item{
  background: #fff;
  padding: 40rpx;
  margin: 20rpx 0px;
}
.code-item-inner{
  overflow: hidden;
  display:flex;
  align-items:center;
  justify-content:space-between;
}
.code-text{
  font-weight: bold;
  font-size: 56rpx;
  color: #08c;
}
.code-text-info{
  font-size: 22rpx;
  color: #555;
}